{
  "name": "xutl/yii2-payment",
  "description": "The payment extension for the Yii framework",
  "type": "yii2-extension",
  "license": "MIT",
  "keywords": [
    "yii",
    "payment"
  ],
  "authors": [
    {
      "name": "XuTL",
      "email": "xutl@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-openssl":"*",
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-httpclient": "~2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "xutl\\payment\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "xutl\\payment\\tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
